{
  "name": "curls",
  "version": "0.4.1",
  "main": "es/index",
  "module": "es/index",
  "jsnext:main": "es/index",
  "esnext": "src/index",
  "private": false,
  "dependencies": {
    "cancelable-promise": "^2.3.0",
    "fast-memoize": "^2.2.8",
    "react": "^16.0.0-rc.3",
    "react-dom": "^16.0.0-rc.3",
    "react-immutable-proptypes": "^2.1.0",
    "webpack": "4.0.0-alpha.0"
  },
  "peerDependencies": {
    "immutable": "^3.8.1",
    "react": ">=15.x",
    "react-dom": ">=15.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.34",
    "@babel/core": "^7.0.0-beta.34",
    "@babel/plugin-proposal-do-expressions": "^7.0.0-beta.35",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.35",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0-beta.35",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.35",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-beta.35",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.35",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.35",
    "@babel/polyfill": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "@babel/preset-react": "^7.0.0-beta.34",
    "@babel/preset-stage-2": "^7.0.0-beta.35",
    "@babel/register": "^7.0.0-beta.34",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-pure-components": "^3.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "group-css-media-queries": "^1.4.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaredlunde/curls"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node server.js",
    "build": "yarn run build:commonjs && yarn run build:es && yarn run build:dist",
    "build:commonjs": "rm -rf commonjs && cross-env NODE_ENV=commonjs babel ./src --out-dir commonjs",
    "build:es": "rm -rf es && cross-env NODE_ENV=es babel ./src --out-dir es",
    "build:dist": "rm -rf dist && cross-env NODE_ENV=production webpack --mode production --config webpack.config.js",
    "node": "cross-env NODE_ENV=cli babel-node",
    "watch:commonjs": "cross-env NODE_ENV=commonjs babel ./src -w --out-dir commonjs",
    "watch:es": "cross-env NODE_ENV=es babel ./src -w --out-dir es",
    "prepublish": "yarn run build",
    "analyze": "cross-env NODE_ENV=production webpack --config webpack.config.js -j | webpack-bundle-size-analyzer"
  },
  "keywords": [
    "ui-framework",
    "ui",
    "react",
    "react-components",
    "react-component",
    "responsive",
    "css",
    "sass",
    "frontend-framework",
    "frontend",
    "sass-framework"
  ],
  "authors": [
    "Jared Lunde <jared@tessellate.io> (https://github.com/jaredlunde)"
  ],
  "analyze": true,
  "sideEffects": false,
  "license": "MIT"
}
